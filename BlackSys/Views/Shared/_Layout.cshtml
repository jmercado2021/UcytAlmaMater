<body class="page-header-fixed page-sidebar-closed-hide-logo page-content-white">
    @RenderSection("topLevel", required: false)
    <!--HEADER-->
    <div class="page-header navbar navbar-fixed-top">

        <div class="page-header-inner">
            <div class="page-logo left">
              
                <a href="/">
                    <img style="width: 200px; height: 46px;" src="~/Imagenes/LogoUcyt.png" class="img-fluid" alt="Responsive image">
                    @*<img src="~/Imagenes/LogoOficina1.jpg"  alt="logo" class="logo-default"/>*@
