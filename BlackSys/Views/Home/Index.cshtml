

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
    var user = ViewBag.user;
    string greeting = "Bienvenido " + user;
}

<script>
    // Asegúrate de que la variable 'greeting' esté definida correctamente
    var greeting = @Html.Raw(Json.Encode(greeting));

    alertify.success(greeting);
</script>